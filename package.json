{
  "name": "kafka-lens",
  "version": "2.0.0",
  "description": "A tool for visualizing a Kafka cluster's brokers, topics, partitions, and message flow.",
  "main": "main/main.js",
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "scripts": {
    "dev": "electron . --inspect=5858",
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "postinstall": "electron-builder install-app-deps",
    "start:dev": "webpack-dev-server --open",
    "webpack": "webpack",
    "lint": "eslint '**/*.{js,jsx}'",
    "lint:fix": "eslint --fix '**/*.{js,jsx}'",
    "test": "jest",
    "test:frontend": "jest client",
    "test:backend": "jest main",
    "coverage:backend": "jest --collectCoverageFrom=main/kafka/**.js --coverage main",
    "package": "",
    "build": "electron-packager . Kafka-Lens",
    "build-win": "electron-packager . kafka-lens --platform win32 --arch x64 --out dist/",
    "setup-win": "electron-installer-windows --src dist/kafka-lens-win32-x64/ --dest dist/installers/",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=true --out=Launch-application",
    "package-win": "electron-packager . Kafka-Lens --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=Launch-application --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Kafka Lens\"",
    "package-linux": "electron-packager . Kafka-Lens --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/icon.png --prune=true --out=Launch-application"
  },
  "build": {
    "appId": "kafka lens",
    "productName": "Kafka Lens"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/teampandawhale/kafka-lens.git"
  },
  "keywords": [
    "kafka"
  ],
  "author": "TEAM PANDA WHALE",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/teampandawhale/kafka-lens/issues"
  },
  "homepage": "https://github.com/teampandawhale/kafka-lens#readme",
  "dependencies": {
    "chart.js": "^2.8.0",
    "electron": "^4.2.9",
    "jest-css-modules-transform": "^2.1.1",
    "kafka-node": "^4.0.1",
    "node-sass": "^4.11.0",
    "node-zookeeper-client": "^1.0.0",
    "react": "^16.8.1",
    "react-chartjs-2": "^2.7.6",
    "react-dom": "^16.8.1",
    "react-router-dom": "^5.0.1",
    "sass-loader": "^7.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.1.0",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.0",
    "electron-builder": "^21.1.5",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.9.1",
    "enzyme-to-json": "^3.3.5",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.0.0",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^1.7.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.1.0",
    "prettier": "^1.18.2",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.29.3",
    "webpack-cli": "^3.2.3",
    "webpack-dev-server": "^3.1.14"
  }
}
